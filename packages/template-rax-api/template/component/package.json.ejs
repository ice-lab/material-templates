{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "main": "lib/index.js",
  "module": "es/index.js",
  "exports": {
    ".":{
      <%_ projectTargets.forEach(function(target) { _%>
    "<%= target %>": "./es/<%= target %>/index.js",
      <%_ })_%>
      "default": "./es/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "build",
    "dist",
    "es",
    "lib"
  ],
  "keywords": [
    "Rax",
    "api"
  ],
  "engines": {
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "build-scripts start",
    "build": "build-scripts build",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "typescript": "^3.7.5",
    "eslint": "^6.8.0",
    "build-plugin-rax-component": "^0.2.0",
    "@alib/build-scripts": "^0.1.0"
  },
  "componentConfig": {
    "name": "<%= name %>",
    "title": "<%= title %>",
    "category": "<%= category %>"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
