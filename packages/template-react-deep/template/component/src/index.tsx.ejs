import React from 'react';
import { ConfigProvider, Button } from '@ali/deep';
import PropTypes from 'vision-types';

interface IProps {
  deepPrefix: string,
  device: string
}

class Deep<%= className %> extends React.Component<IProps> {
  static defaultProps = {
    deepPrefix: 'deep-',
    device: 'desktop'
  }
  // for low-code
  static propTypes = {
    deepPrefix: PropTypes.string,
    device: PropTypes.string
  }

  render() {
    const { deepPrefix } = this.props;
    return (
      <div className={`${deepPrefix}<%= kebabCaseName %>`}>
        Hello <%= className %>
        <Button type={'primary'}>确定</Button>
      </div>
    );
  }
}

export default ConfigProvider.config(Deep<%= className %>, {});
