{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    ".vscode/",
    "src/",
    "build/",
    "public/",
    "_gitignore",
    "_editorconfig",
    "_eslintignore",
    "_eslintrc.js",
    "build.json",
    "screenshot.png",
    "tsconfig.json",
    "jsconfig.json"
  ],
  "scripts": {
    "start": "rax-app start",
    "build": "rax-app build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "screenshot": "screenshot -l -u /build/web/index.html -s root",
    "prepublishOnly": "npm run build && npm run screenshot"
  },
  "dependencies": {
    "rax": "^1.1.0",
    "rax-document": "^0.1.0",
    "rax-image": "^2.0.0",
    "rax-link": "^1.0.1",
    "rax-text": "^2.0.0",
    "rax-view": "^2.0.0"
  },
  "devDependencies": {
    "rax-app": "^3.0.0",
    "@iceworks/spec": "^1.0.0",
    "@types/rax": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0"
  },
  "scaffoldConfig": {
    "name": "<%= name %>",
    "title": "<%= title %>",
    "category": "<%= category %>"
  },
  "publishConfig": {
    "access": "public"
  }
}
