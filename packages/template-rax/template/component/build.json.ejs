{
  "type": "rax",
  "targets": ["<%- projectTargets.join('", "') %>"],
  <%_ if (miniappComponentBuildType === 'runtime') {_%>
    <%_ projectTargets.forEach(function(target) { _%>
      <%_ if (['miniapp', 'wechat-miniprogram'].includes(target)) {_%>
  "<%= target %>": {
    "buildType": "runtime"
  },
      <%_ }_%>
    <%_ })_%>
  <%_ } _%>
  "plugins": [
    "build-plugin-component"
    <% if(isAliInternal && projectTargets.includes('miniapp')) {%>
      ,"@ali/build-plugin-miniapp-preview"
    <%}%>
  ]
}
