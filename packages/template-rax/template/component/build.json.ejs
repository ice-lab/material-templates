{
  "type": "rax",
  "targets": ["<%- projectTargets.join('", "') %>"],
  <%_ if (typeof miniappComponentBuildType !== 'undefined' && miniappComponentBuildType === 'runtime') {_%>
    <%_ projectTargets.forEach(function(target) { _%>
      <%_ if (['miniapp', 'wechat-miniprogram'].includes(target)) {_%>
  "<%= target %>": {
    "buildType": "runtime"
  },
      <%_ }_%>
    <%_ })_%>
  <%_ } _%>
  "plugins": [
    "build-plugin-component"
    <% if(typeof isAliInternal !== 'undefined' && isAliInternal && projectTargets.includes('miniapp')) {%>
      ,"@ali/build-plugin-miniapp-preview"
    <%}%>
  ],
  "enablePlatformLoader": true
}
