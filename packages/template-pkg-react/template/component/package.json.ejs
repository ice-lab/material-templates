{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    "demo/",
    "esm/",
    "cjs/",
    "es2017",
    "build/"
  ],
  "module": "esm/index.js",
  "exports": {
    "es2017": "./es2017/index.js",
    "import": "./esm/index.js"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "@swc/helpers": "^0.3.8"
  },
  "devDependencies": {
    "@ice/pkg": "^1.0.0-rc.0",
    "@ice/pkg-plugin-docusaurus": "^1.0.0-rc.0",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stylelint": "^13.7.2"
  },
  "peerDependencies": {
    "react": "^16 || ^17"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
