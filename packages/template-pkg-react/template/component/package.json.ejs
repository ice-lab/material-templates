{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    "esm",
    "cjs",
    "es2017",
    "dist",
    "build"
  ],
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": "./es2017/index.js",
      "default": "./esm/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@ice/pkg-plugin-docusaurus": "^1.0.0",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "style-unit": "^3.0.4",
    "stylelint": "^13.7.2"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
