{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./es2017/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js",
      },
      "import": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      }
    },
    "./*": "./*"
  },
  "files": [
    "esm",
    "cjs",
    "es2017"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "keywords": [
    "icepkg",
    "js",
    "library"
  ],
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
