{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist"
  ],
  "type": "module",
  "main": "es2017/index.js",
  "module": "es2017/index.js",
  "types": "es2017/index.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "keywords": [
    "icepkg",
    "js",
    "node",
    "library"
  ],
  "dependencies": {
    "@swc/helpers": "^0.5.1"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@ice/pkg": "^1.0.0",
<% if (!locals.workspace) { _%>
    "@ice/pkg-plugin-docusaurus": "^1.0.0",
<% } _%>
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "eslint": "^8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
