{
  "name": "<%= npmName %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist"
  ],
  "type": "module",
  "main": "es2017/index.js",
  "module": "es2017/index.js",
  "types": "es2017/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "scripts": {
    "start": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "keywords": [
    "icepkg",
    "js",
    "node",
    "library"
  ],
  "devDependencies": {
    "@ice/pkg": "^1.0.0",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
